    while i + 1 < len(A) and A[i] < A[i+1]:
        i += 1
    # Peak can't be first or last
    if i == 0 or i == len(A) - 1:
        return False
    # Walk down
    while i + 1 < len(A) and A[i] > A[i+1]:
        i += 1
    return i == len(A) - 1
