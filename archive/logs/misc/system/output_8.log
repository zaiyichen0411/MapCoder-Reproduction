class ListNode:
    def __init__(self, val=0, next=None):
        self.val = val
        self.next = next

def rotateRight(head: ListNode, k: int) -> ListNode:
    if not head or not head.next or k == 0:
        return head

    # Find length of the list
    length = 1
    current = head
    while current.next:
        length += 1
        current = current.next

    # Adjust k
    k = k % length
    if k == 0:
        return head

    # Find the new head
    new_head = head
    for _ in range(length - k - 1):
        new_head = new_head.next

    # Split the list
    result = new_head.next
    new_head.next = None

    # Find the end of the new list and connect to original head
    current = result
    while current.next:
        current = current.next
    current.next = head

    return result
```

